{
  "name": "mailosaur/mailosaur-php",
  "description": "The Mailosaur PHP library lets you integrate email and SMS testing into your continuous integration process.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Mailosaur\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MailosaurTest\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "nette/utils": "v3.0.1",
    "nette/mail": "v3.1.0"
  },
  "keywords": [
    "mailosaur",
    "email",
    "sms",
    "testing",
    "automation"
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mailosaur",
      "email": "code@mailosaur.com"
    }
  ]
}
